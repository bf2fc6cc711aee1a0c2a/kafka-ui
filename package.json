{
  "name": "kafka-ui",
  "federatedModuleName": "kafka",
  "private": true,
  "version": "0.0.1",
  "description": "Strimzi UI provides a way for managing Strimzi and Kafka clusters (+ other components) deployed by it using a graphical user interface.",
  "scripts": {
    "api:generate": "openapi-generator-cli generate -i openapi/kafka-admin.yaml -g typescript-axios -o src/OpenApi --additional-properties=ngVersion=6.1.7,npmName=restClient,supportsES6=true,npmVersion=6.9.0,withInterfaces=true",
    "clean": "rm -rf dist/ && rm -rf js/ && rm -rf .out/ && rm -rf generated/ && rm -rf coverage/ && npx jest --clear-cache",
    "start:restmockserver": "node mock-server/src/index.js",
    "start": "npm-run-all --parallel --race start:client start:restmockserver",
    "start:client": "webpack serve --config webpack.config.js",
    "build": "webpack --config webpack.config.js --mode production",
    "build:bundle-profile": "webpack --config webpack.config.js --mode production --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "pretest": "prettier --config ./linting/prettier.config.js --check .",
    "test": "npm-run-all test:jest",
    "test:jest": "npm-run-all \"test:jest:* -- {1}\" --",
    "test:jest:client": "jest --collectCoverage --config src/jest.config.js",
    "test:e2e": "npm-run-all clean build --parallel --race start:e2eserver cy:closed",
    "test:e2e:open": "npm-run-all clean build --parallel --race start:e2eserver cy:open",
    "lint": "npm-run-all 'lint:src -- .' 'lint:styling -- \"**/*.(s)css\"' 'lint:format -- .'",
    "lint:src": "eslint --config ./linting/eslint.config.js --fix --max-warnings 0 --ignore-path ./linting/.eslintignore",
    "lint:styling": "stylelint --config ./linting/stylelint.config.js --fix --max-warnings 0",
    "lint:format": "prettier --config ./linting/prettier.config.js --write",
    "cy:open": "cypress open --config-file e2e/cypress.json",
    "cy:closed": "cypress run --config-file e2e/cypress.json"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strimzi/strimzi-ui.git"
  },
  "license": "Apache-2.0",
  "bugs": {},
  "homepage": "https://github.com/strimzi/strimzi-ui#readme",
